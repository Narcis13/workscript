{
    "id": "test-nodes",
    "name": "Simple Test Workflow",
    "version": "1.0.0",
    "description": "Generate token and save to file!",
    "initialState": {
      "test_data":

      [
        {
          "name": "John Doe",
          "email": "john.doe@example.com",
          "phone": "1234567890"
        },
        {
          "name": "Jane Doe",
          "email": "jane.doe@example.com",
          "phone": "0987654321"
        }
      ]
    },
    "workflow": [
         {
              "filter": {
                "items": "$.test_data",
                "conditions": [
                  {
                    "field": "name",
                    "dataType": "string",
                    "operation": "equals",
                    "value": "John Doe"
                  }
                ],
               "matchMode": "all"
          
           }
         },
      "empty"
    ]
  }